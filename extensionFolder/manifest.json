{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "permissions": [
  "webRequest",
  "webRequestBlocking",
  "tabs",
  "<all_urls>"
  ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
],
"background": {
    "scripts": ["content.js"],
    "persistent": true
  }

}
